define([
    'marionette'

], function (Marionette, GameTemplate) {

    var Views = {};

    Views.Layouts = Marionette.Layout.extend({

        template       : StartGameTemplate,
        submitItemList : true,
        regions        : {
            startGame     : '.js-start-game'
        },
        events         : {
            'click .js-start-game'             : 'startLevel'
        },

        startLevel : function (level) {
            console.log(level);
        }
    });

    Views.CreateItemListView = Marionette.CompositeView.extend({
        template          : StartLevelTemplate,
        itemView          : Views.Layouts,
        itemViewContainer : '.js-table-content'
    });

    return Views;
});